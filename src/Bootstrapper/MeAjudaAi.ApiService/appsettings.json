{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Extensions.Http": "Warning",
        "System.Net.Http": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {CorrelationId} {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "MeAjudaAi"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "ServiceName": "MeAjudaAi-api",
    "ServiceVersion": "1.0.0"
  },
  "AdvancedRateLimit": {
    "Anonymous": {
      "RequestsPerMinute": 30,
      "RequestsPerHour": 300,
      "RequestsPerDay": 1000
    },
    "Authenticated": {
      "RequestsPerMinute": 120,
      "RequestsPerHour": 2000,
      "RequestsPerDay": 10000
    },
    "General": {
      "Enabled": true,
      "WindowInSeconds": 60,
      "EnableIpWhitelist": false,
      "WhitelistedIps": [],
      "EnableDetailedLogging": true,
      "ErrorMessage": "Limite de requisições excedido. Tente novamente mais tarde."
    }
  },
  "Caching": {
    "DefaultExpirationMinutes": 30,
    "RedisConnectionString": ""
  },
  "Keycloak": {
    "BaseUrl": "http://localhost:8080",
    "Realm": "meajudaai",
    "ClientId": "meajudaai-api",
    "RequireHttpsMetadata": true
  },
  "Messaging": {
    "Enabled": false,
    "Provider": "ServiceBus",
    "ServiceBus": {
      "ConnectionString": "",
      "DefaultTopicName": "MeAjudaAi-events",
      "Strategy": "Hybrid",
      "AutoCreateTopics": true,
      "DomainTopics": {
        "Users": "users-events"
      }
    },
    "RabbitMQ": {
      "ConnectionString": "",
      "DefaultQueueName": "MeAjudaAi-events",
      "Host": "localhost",
      "Port": 5672,
      "Username": "guest",
      "Password": "guest",
      "VirtualHost": "/",
      "Strategy": "Hybrid",
      "DomainQueues": {
        "Users": "users-events"
      }
    }
  },
  "Cache": {
    "WarmupEnabled": false,
    "WarmupTimeoutSeconds": 30
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:3000", "http://localhost:5173" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH" ],
    "AllowedHeaders": [ "*" ]
  },
  "Locations": {
    "ExternalApis": {
      "ViaCep": {
        "BaseUrl": "https://viacep.com.br"
      },
      "BrasilApi": {
        "BaseUrl": "https://brasilapi.com.br"
      },
      "OpenCep": {
        "BaseUrl": "https://opencep.com"
      },
      "Nominatim": {
        "BaseUrl": "https://nominatim.openstreetmap.org/",
        "UserAgent": "MeAjudaAi/1.0 (https://github.com/frigini/MeAjudaAi)"
      },
      "IBGE": {
        "BaseUrl": "https://servicodados.ibge.gov.br/api/v1/localidades/"
      }
    }
  },
  "FeatureManagement": {
    "GeographicRestriction": false
  }
}