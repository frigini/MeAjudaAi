{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "ServiceName": "meajudaai-api",
    "ServiceVersion": "1.0.0"
  },
  "Postgres": {
    "ConnectionString": "Host=localhost;Database=meajudaai;Username=postgres;Password=postgres;Port=5432"
  },
  "RateLimit": {
    "DefaultRequestsPerMinute": 60,
    "AuthRequestsPerMinute": 5,
    "SearchRequestsPerMinute": 100,
    "WindowInSeconds": 60
  },
  "Caching": {
    "DefaultExpirationMinutes": 30,
    "RedisConnectionString": ""
  },
  {
    "Messaging": {
      "ServiceBus": {
        "ConnectionString": "",
        "DefaultTopicName": "meajudaai-events",
        "Strategy": "Hybrid",
        "AutoCreateTopics": true,
        "DomainTopics": {
          "Users": "users-events"
          //"ServiceProvider": "serviceprovider-events",
          //"Customer": "customer-events",
          //"Billing": "billing-events",
          //"Notification": "notification-events"
        }
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:3000", "http://localhost:5173" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH" ],
    "AllowedHeaders": [ "*" ]
  },
  "ApiVersioning": {
    "DefaultVersion": "1.0",
    "AssumeDefaultVersionWhenUnspecified": true
  }
}