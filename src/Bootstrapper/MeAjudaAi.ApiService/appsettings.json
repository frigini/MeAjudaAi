{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.Authorization": "Debug"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "ServiceName": "MeAjudaAi-api",
    "ServiceVersion": "1.0.0"
  },
  "Postgres": {
    "ConnectionString": "Host=localhost;Database=MeAjudaAi;Username=postgres;Password=postgres;Port=5432"
  },
  "RateLimit": {
    "DefaultRequestsPerMinute": 60,
    "AuthRequestsPerMinute": 5,
    "SearchRequestsPerMinute": 100,
    "WindowInSeconds": 60
  },
  "Caching": {
    "DefaultExpirationMinutes": 30,
    "RedisConnectionString": ""
  },
  "Keycloak": {
    "BaseUrl": "http://localhost:8080",
    "Realm": "meajudaai",
    "ClientId": "meajudaai-api",
    "RequireHttpsMetadata": false
  },
  "Messaging": {
    "ServiceBus": {
      "ConnectionString": "",
      "DefaultTopicName": "MeAjudaAi-events",
      "Strategy": "Hybrid",
      "AutoCreateTopics": true,
      "DomainTopics": {
        "Users": "users-events"
      }
    },
    "RabbitMQ": {
      "ConnectionString": "",
      "DefaultQueueName": "MeAjudaAi-events",
      "Host": "localhost",
      "Port": 5672,
      "Username": "guest",
      "Password": "guest",
      "VirtualHost": "/",
      "Strategy": "Hybrid",
      "DomainQueues": {
        "Users": "users-events"
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:3000", "http://localhost:5173" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH" ],
    "AllowedHeaders": [ "*" ]
  },
  "ApiVersioning": {
    "DefaultVersion": "1.0",
    "AssumeDefaultVersionWhenUnspecified": true
  }
}