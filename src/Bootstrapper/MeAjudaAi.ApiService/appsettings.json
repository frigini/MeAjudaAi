{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Extensions.Http": "Warning",
        "System.Net.Http": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {CorrelationId} {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "MeAjudaAi"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Warning",
      "Microsoft.AspNetCore.Authorization": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "ServiceName": "MeAjudaAi-api",
    "ServiceVersion": "1.0.0"
  },
  "RateLimit": {
    "DefaultRequestsPerMinute": 60,
    "AuthRequestsPerMinute": 5,
    "SearchRequestsPerMinute": 100,
    "WindowInSeconds": 60
  },
  "Caching": {
    "DefaultExpirationMinutes": 30,
    "RedisConnectionString": ""
  },
  "Keycloak": {
    "BaseUrl": "http://localhost:8080",
    "Realm": "meajudaai",
    "ClientId": "meajudaai-api",
    "RequireHttpsMetadata": true
  },
  "Messaging": {
    "Enabled": false,
    "Provider": "ServiceBus",
    "ServiceBus": {
      "ConnectionString": "",
      "DefaultTopicName": "MeAjudaAi-events",
      "Strategy": "Hybrid",
      "AutoCreateTopics": true,
      "DomainTopics": {
        "Users": "users-events"
      }
    },
    "RabbitMQ": {
      "ConnectionString": "",
      "DefaultQueueName": "MeAjudaAi-events",
      "Host": "localhost",
      "Port": 5672,
      "Username": "guest",
      "Password": "guest",
      "VirtualHost": "/",
      "Strategy": "Hybrid",
      "DomainQueues": {
        "Users": "users-events"
      }
    }
  },
  "Cache": {
    "WarmupEnabled": false,
    "WarmupTimeoutSeconds": 30
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:3000", "http://localhost:5173" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH" ],
    "AllowedHeaders": [ "*" ]
  }
}