<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="betenbough-feed" value="https://pkgs.dev.azure.com/betenboughsystems/main/_packaging/betenbough-feed/nuget/v3/index.json" />
  </packageSources>
  
  <packageSourceMapping>
    <!-- Mapear todos os pacotes padrão para nuget.org -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    
    <!-- Mapear apenas pacotes específicos para betenbough-feed (se houver) -->
    <packageSource key="betenbough-feed">
      <!-- Padrão genérico para evitar erro de configuração -->
      <package pattern="Betenbough.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>